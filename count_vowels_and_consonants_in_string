public class Main
{
	public static void main(String[] args)
	{
		System.out.println("Hello World");
		
		String str1 = "Hello kanupriya Gupta";
		count_vowels_consonets_in_string(str1);

	}

    public static void count_vowels_consonets_in_string(String text)
    {
        int vowels = 0;
        int consonets = 0;
        
        
        for(int i=0; i<text.length(); i++)
        {
            if(text.charAt(i) == 'a' ||
                text.charAt(i) == 'e' ||
                text.charAt(i) == 'i' || 
                text.charAt(i) == 'o' || 
                text.charAt(i) == 'u')
                
                vowels++;
            else 
                consonets++;
        }
        
        
        System.out.println("count of vowels    : " + vowels);
        System.out.println("count of consonets : " + consonets);
    }
}
